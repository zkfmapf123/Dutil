{
  "name": "ts-dkutil",
  "description": "donggyu utils use typescript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "version": "1.0.2",
  "license": "MIT",
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "clean": "rimraf -rf lib && mkdirp lib",
    "build": "npm run build-dev",
    "build-prod": "npm run clean && tsc && cross-env NODE_ENV=prod babel ./src --ignore */test --out-dir lib --extensions \".ts,.tsx\"",
    "build-dev": "npm run clean && tsc && cross-env NODE_ENV=dev babel ./src --ignore */test --out-dir lib --extensions \".ts,.tsx\" ",
    "build-watch": "npm run clean && babel -w ./src --out-dir lib --extensions \".ts,.tsx\"",
    "test": "jest --verbose --testTimeout=10000",
    "release": "npm run build && NODE_ENV=\"test\" npx np --any-branch",
    "pre-release": "npm run test && npm run build && npm publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/hapi__joi": "^16.0.4",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.116",
    "@types/moment-range": "^3.1.2",
    "@types/moment-timezone": "^0.5.12",
    "@types/node-fetch": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.4",
    "firebase-functions-test": "^0.2.0",
    "jest": "^27.5.1",
    "mkdirp": "^1.0.3",
    "np": "^6.5.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3"
  },
  "author": "leedonggyu"
}
